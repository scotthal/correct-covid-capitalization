{
  "name": "Correct COVID Capitalization",
  "description": "Correctly capitalize 'COVID.'",
  "version": "1.0.0",
  "manifest_version": 2,
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": [
        "http://*.washingtonpost.com/*",
        "https://*.washingtonpost.com/*"
      ],
      "js": ["content.js"]
    }
  ],
  "options_page": "options.html"
}
